[email]
SmtpServer = yourmailserver.com
SmtpPort = 587
SmtpUser = user@yourmailserver.com
SmtpPassword = yourpassword
SmtpSender = noreply@yourmailserver.com
MailRecipient = recipient@yourmailserver.com
MailPrefix = Recurring Task
;EncryptionMode: 0 = No encryption, 1 = SSL, 2 = StartTLS
EncryptionMode = 2
;Set NoLogin to 1 if your smtp server does not require authentication
NoLogin = 0
